{
  "name": "@semantikit/react",
  "version": "0.1.0-alpha.1",
  "description": "React components and hooks for SemantiKit knowledge graphs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --external react --external react-dom --external reactflow",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external react-dom --external reactflow --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "react",
    "knowledge-graph",
    "graph-visualization",
    "hooks",
    "components"
  ],
  "author": "Garrett Eastham",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "reactflow": "^11.0.0"
  },
  "dependencies": {
    "@semantikit/core": "workspace:*",
    "@semantikit/validators": "workspace:*",
    "@semantikit/layouts": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
