{
  "name": "@semantikit/validators",
  "version": "0.1.0-alpha.1",
  "description": "Schema validation utilities for SemantiKit knowledge graphs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "validation",
    "schema",
    "json-schema",
    "data-validation"
  ],
  "author": "Garrett Eastham",
  "license": "MIT",
  "dependencies": {
    "@semantikit/core": "workspace:*",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

